{
    "$schema": "../node_modules/@tauri-apps/cli/schema.json",
    "build": {
        "beforeBuildCommand": "pnpm run build",
        "beforeDevCommand": "pnpm run dev",
        "devPath": "http://localhost:5258",
        "distDir": "../.solid"
    },
    "package": {
        "productName": "emberry",
        "version": "0.1.0"
    },
    "tauri": {
        "allowlist": {
            "clipboard": {
                "writeText": true,
                "readText": false
            }
        },
        "bundle": {
            "active": true,
            "category": "DeveloperTool",
            "copyright": "",
            "deb": {
                "depends": []
            },
            "externalBin": [],
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "identifier": "com.emberry.app",
            "longDescription": "",
            "macOS": {
                "entitlements": null,
                "exceptionDomain": "",
                "frameworks": [],
                "providerShortName": null,
                "signingIdentity": null
            },
            "resources": [],
            "shortDescription": "",
            "targets": "all",
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": ""
            }
        },
        "security": {
            "csp": null
        },
        "updater": {
            "active": true,
            "endpoints": [
                "https://github.com/emberry-org/emberry/releases/latest/download/latest.json"
            ],
            "dialog": false,
            "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDMwMjFDMzUyREQ2MkJGRTYKUldUbXYyTGRVc01oTUxpWXQvK0FjZE1BNGpVZXpoWXdQRGRQbjFZTGI2L1JtOGdZWnh6VlV2ZVoK",
            "windows": {
                "installMode": "quiet"
            }
        },
        "windows": [
            {
                "fullscreen": false,
                "height": 600,
                "resizable": true,
                "title": "Emberry",
                "width": 800
            }
        ]
    }
}